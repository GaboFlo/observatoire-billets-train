{
  "name": "observatoire-billets-train",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "front",
    "back"
  ],
  "scripts": {
    "install:all": "npm install",
    "build:all": "cd front && npm run build && cd ../back && npm run build",
    "dev:front": "cd front && npm run dev",
    "dev:back": "cd back && npm run dev",
    "dev": "concurrently \"npm run dev:front\" \"npm run dev:back\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}